 void find_repeated_digit(int *arr, int len)
{ int *start , *last = arr + len - 1;
for ( start = arr + 1; arr < last; arr++, start = arr + 1 )
 { while ( start <= last )
   {if ( *start == *arr )
            {*start = *last--;
            }
            else
            {
                start++;
            }
        }
    }
}

